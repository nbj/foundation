    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "nbj/strings": "*",
        "nbj/collections": "*",
        "nbj/config": "*"
    },
    "require-dev": {
